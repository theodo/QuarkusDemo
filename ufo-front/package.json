{
  "name": "seed",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.7.1",
    "@sentry/browser": "^5.4.3",
    "@types/google-map-react": "^1.1.3",
    "@types/material-ui": "^0.21.7",
    "@types/react-lottie": "^1.2.3",
    "babel-polyfill": "6.26.0",
    "connected-react-router": "^6.5.2",
    "env-cmd": "8.0.2",
    "formik": "1.3.2",
    "google-map-react": "^1.1.5",
    "history": "4.7.2",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.15",
    "node-sass": "^4.13.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-intl": "2.7.2",
    "react-lottie": "^1.2.3",
    "react-redux": "^7.1.3",
    "react-router": "5.0.1",
    "react-router-dom": "5.0.1",
    "react-scripts": "^3.1.1",
    "redux": "4.0.4",
    "redux-persist": "5.10.0",
    "redux-saga": "^1.0.2",
    "styled-components": "4.1.1",
    "superagent": "4.0.0",
    "typesafe-actions": "^3.0.0"
  },
  "scripts": {
    "audit-dependencies": "audit-ci --config .audit-ci.json",
    "start": "REACT_APP_ENV=local react-app-rewired start",
    "build": "env-cmd .env.${REACT_APP_ENV} react-app-rewired build",
    "test": "react-app-rewired test --maxWorkers=4",
    "eject": "react-app-rewired eject",
    "lint": "eslint 'src/**/*.{js,ts,tsx}'",
    "lint:fix": "eslint 'src/**/*.{js,ts,tsx}' --fix",
    "lint-style": "stylelint 'src/**/*.style.{ts,tsx}' 'src/**/style.{ts,tsx}'",
    "type-check": "tsc"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.test.js",
      "!src/**/actions.js",
      "!src/**/index.js",
      "!src/**/serviceWorker.js",
      "!src/**/*.wrap.js",
      "!src/index.js",
      "!src/tempPolyfills.js",
      "!src/setupTests.js",
      "!src/redux/reducers.js",
      "!src/redux/sagas.js",
      "!src/redux/store.js"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 0,
        "branches": 0,
        "functions": 0,
        "lines": 0
      }
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ]
  },
  "bundlesize": [
    {
      "path": "./build/static/js/*.js",
      "maxSize": "200 kB"
    }
  ],
  "devDependencies": {
    "@types/cheerio": "^0.22.10",
    "@types/enzyme": "^3.1.15",
    "@types/history": "^4.7.2",
    "@types/jest": "^24.0.18",
    "@types/jwt-decode": "^2.2.1",
    "@types/lodash": "^4.14.119",
    "@types/node": "^10.12.18",
    "@types/react": "^16.9.14",
    "@types/react-dom": "^16.9.4",
    "@types/react-intl": "^2.3.15",
    "@types/react-redux": "^7.1.5",
    "@types/react-router": "^5.0.3",
    "@types/react-router-dom": "^4.3.5",
    "@types/react-test-renderer": "^16.9.1",
    "@types/styled-components": "^4.1.5",
    "@types/superagent": "^3.8.6",
    "@types/webpack-env": "^1.14.0",
    "@typescript-eslint/eslint-plugin": "^2.5.0",
    "@typescript-eslint/parser": "^2.5.0",
    "audit-ci": "^2.4.0",
    "babel-plugin-styled-components": "1.8.0",
    "bundlesize": "^0.18.0",
    "cypress": "^3.4.1",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.15.1",
    "enzyme-to-json": "3.4.3",
    "eslint-config-prettier": "6.2.0",
    "eslint-config-react-app": "^5.0.1",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jest": "22.17.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-mysticatea": "4.2.4",
    "eslint-plugin-prefer-object-spread": "1.2.1",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react": "7.14.3",
    "eslint-plugin-risxss": "^1.1.1",
    "plop": "2.1.0",
    "prettier": "1.15.1",
    "react-app-rewire-webpack-bundle-analyzer": "^1.1.0",
    "react-app-rewired": "2.1.0",
    "react-test-renderer": "16.12.0",
    "redux-saga-test-plan": "^4.0.0-beta.2",
    "stylelint": "^15.10.1",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-declaration-strict-value": "^1.1.2",
    "stylelint-processor-styled-components": "^1.5.2",
    "typescript": "^3.2.2",
    "wait-on": "^3.2.0",
    "webpack-manifest-plugin": "^2.0.4"
  }
}
